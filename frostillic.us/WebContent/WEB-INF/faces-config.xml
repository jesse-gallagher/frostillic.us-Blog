<faces-config>
  <managed-bean>
    <managed-bean-name>flashScope</managed-bean-name>
    <managed-bean-class>java.util.HashMap</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>akismet</managed-bean-name>
    <managed-bean-class>frostillicus.Akismet</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>gaAccount</managed-bean-name>
    <managed-bean-class>frostillicus.GoogleAnalyticsAccount</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>applicationDispatcher</managed-bean-name>
    <managed-bean-class>frostillicus.event.SimpleEventDispatcher</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
    <managed-property>
      <property-name>listenerClasses</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>frostillicus.Messenger</value>
      </list-entries>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>sessionDispatcher</managed-bean-name>
    <managed-bean-class>frostillicus.event.SimpleEventDispatcher</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>listenerClasses</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
      </list-entries>
    </managed-property>
  </managed-bean>
  <!-- GitHub REST API -->
  <managed-bean>
    <managed-bean-name>GitHub</managed-bean-name>
    <managed-bean-class>com.ibm.xsp.extlib.sbt.services.client.endpoints.BasicEndpointBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
    <managed-property>
      <property-name>url</property-name>
      <value>https://api.github.com</value>
    </managed-property>
  </managed-bean>
